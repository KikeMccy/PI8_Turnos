<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pi8_turnos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/turno"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PI8_Turnos">
        <activity android:name=".TurnosProgramadosActivity"></activity>
        <activity android:name=".AgregarTurnosActivity" />
        <activity android:name=".Calendario_Activity" />
        <activity android:name=".RecuperarPasswordActivity" />
        <activity android:name=".ModificarUserActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".RegistroActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".Activity_entidades" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>